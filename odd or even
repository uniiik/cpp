

#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int k=n;
    
    while(k<=n+1/2)
    {
        if(k%2==0)
        {
            cout<<"even number";
            break;
        }
        else
        cout<<"odd number";
        break;
        
        
        
        
        
        k++;
    }
}
