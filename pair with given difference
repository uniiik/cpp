 //arr[] = {5, 20, 3, 2, 50, 80}, n = 78
// Output: Pair Found: (2, 80)
#include<iostream>
using namespace std;
int main()
{
    int arr[6] = {5, 20, 3, 2, 50, 80};
int n=78;
    for(int i=0;i<6;i++)
    {
        for(int j=1;j<=6;j++)
        {
        if(n+arr[i]==arr[j])
        {
            cout<<arr[i]<<" "<<arr[j]<<endl;
        }
        
    }
    }
}
